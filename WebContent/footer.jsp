$(document).ready(function(){

	$('.on_pannel').bind('click',function(e){
		e.preventDefault();

		$('#footer').addClass('on');

		$('#footer').animate({'padding-top': 725}, {
			duration: 500
			, step: function(){

				$("html, body").scrollTop($("#footer").offset().top - 30);

			}
			, complete: function(){

				$('.world_pannel').show();

			}
		});

	});

	$('.off_pannel').bind('click',function(e){
		e.preventDefault();

		$('.world_pannel').hide();

		$('#footer').animate({'padding-top':'0'},300,function(){

			$('#footer').removeClass('on');

		});

		$('.on_pannel').focus();

	});

});

